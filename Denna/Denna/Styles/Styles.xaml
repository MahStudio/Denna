                        Margin="{StaticResource PivotPortraitThemePadding}"
                        Style="{StaticResource TitleContentControlStyle}"
                        Visibility="Collapsed"
                        Content="{TemplateBinding Title}"
                        ContentTemplate="{TemplateBinding TitleTemplate}"/>
